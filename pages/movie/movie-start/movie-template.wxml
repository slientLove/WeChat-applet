<template name="movie-start">
  <view class='start-wrap'>
    <view class='start-img'>
      <block wx:for="{{starts}}" wx:for-item="item">
        <image wx:if="{{item}}" src="/images/icon/star.png"></image>
        <image wx:else="{{item}}" src='/images/icon/none-star.png'></image>
      </block>
    </view>
    <text>{{score}}</text>
  </view>
</template>
